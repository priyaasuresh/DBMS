                <table>
                    <th>FEEDBACK ID</th>
                    <th>FEEDBACK DESCRIPTION</th>
                    <th>RATING</th>
                    <th>PRODUCT ID</th>

                    <% for(var i=0; i < arr.length;i++ ){ %>
